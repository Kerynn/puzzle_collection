<%= form_with url: "/collectors/#{@collector.id}/puzzles", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :pieces_count %>
  <%= form.text_field :pieces_count %>

  <%= form.label :put_together %>
  <%= form.check_box :put_together %>

  <%= form.submit "Create Puzzle" %>
<% end %>
