<%= form_with url: '/collectors', method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :skills_rating %>
  <%= form.text_field :skills_rating %>

  <%= form.label :under_30_yrs %>
  <%= form.check_box :under_30_yrs %>

  <%= form.submit "Create Collector" %>
<% end %>

