<%= form_with url: "/collectors/#{@collector.id}", method: :patch, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %><br>
  <br>

  <%= form.label :skills_rating %>
  <%= form.text_field :skills_rating %><br>
  <br>

  <%= form.label :under_30_yrs %>
  <%= form.check_box :under_30_yrs %><br>
  <br>

  <%= form.submit "Update Collector" %>
<% end %>