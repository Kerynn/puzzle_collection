<%= link_to "Go to Collectors Index", "/collectors" %>
<br>
<%= link_to "Go to Puzzles Index", "/puzzles" %>

<h1>Collectors</h1>
<%= link_to "New Collector", "/collectors/new" %>
<% @collectors.each do |collector| %>
  <h3><p><%= collector.name %></p></h3>
  <%= link_to "Update #{collector.name}", "/collectors/#{collector.id}/edit" %>
  <br>
  <%= link_to "Delete #{collector.name}", "/collectors/#{collector.id}", method: :delete %> 
  <h4><p>Created: <%= collector.created_at %></p></h4>
  <p>Skills Rating: <%= collector.skills_rating %></p>
  <p>Under 30 years old: <%= collector.under_30_yrs %></p>
<% end %>  